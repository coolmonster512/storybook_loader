{
  "name": "react-native-storybook-loader",
  "version": "0.0.0-development",
  "repository": "https://github.com/elderfo/react-native-storybook-loader.git",
  "bugs": "https://github.com/elderfo/react-native-storybook-loader/issues",
  "author": "Chris Gestred <cgetsfred@gmail.com>",
  "license": "MIT",
  "bin": {
    "rnstl": "./dist/rnstl-cli.js"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.0",
    "faker": "^4.1.0",
    "husky": "^0.13.1",
    "jest": "^19.0.2",
    "mock-fs": "^4.0.0",
    "semantic-release": "^6.3.2",
    "uuid": "^3.0.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "dot": "^1.1.1",
    "findup": "^0.1.5",
    "glob": "^7.1.1"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "esw ./",
    "lint:watch": "yarn run lint -- --watch",
    "prestart": "yarn run build",
    "start": "babel-node src/rnstl-cli.js",
    "prestart:dist": "yarn run build",
    "start:dist": "node dist/rnstl-cli.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "webpack --config ./src/build/webpack.config.js",
    "build:precommit": "git add ./dist/rnstl-cli.js && git reset ./output/storyLoader.js && git checkout ./output/storyLoader.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precommit": "yarn run lint && yarn run test && yarn run build && yarn run build:precommit"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": "./src",
      "pattern": "**/*.test.js",
      "outputFile": "./output/storyLoader.js"
    }
  }
}
